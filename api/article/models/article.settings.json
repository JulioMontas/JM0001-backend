{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "name": "Article",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "tittle": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "tittle"
    },
    "content": {
      "type": "text"
    },
    "brief": {
      "type": "string"
    },
    "date": {
      "type": "datetime"
    },
    "photo": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "articles"
    }
  }
}
